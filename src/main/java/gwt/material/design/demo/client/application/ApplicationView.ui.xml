<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m="urn:import:gwt.material.design.client.ui">

    <ui:with field='res' type='gwt.material.design.demo.client.resources.MaterialResources'/>

    <ui:style>
        .button {
            vertical-align: top;
            margin-left: 20px;
        }

        .panel {
            padding-left: 5%;
            width: 100%;
            margin-top: -150px;
            padding: 10px 50px 150px 50px;
            background: #FFF;
            box-sizing: border-box;
            -webkit-box-sizing: border-box;
            -moz-box-sizing: border-box;
        }

        .pointer {
            cursor: pointer;
        }

        main, footer {
            padding-left: 250px;
        }

        .footer {
            margin-top: -50px !important;
        }

        .logo {
            padding: 10px;
            border-bottom: 1px solid #E9E9E9;
        }

        .btnGettingStarted {
            font-size: 0.3em !important;
        }
    </ui:style>

    <g:HTMLPanel>
        <m:MaterialNavBar ui:field="navBar" backgroundColor="blue darken-1">
            <m:MaterialSideNav closeOnClick="false" name="GWT Material" type="OPEN" width="250" waves="default">
                <m:MaterialNavSection align="right">
                    <m:MaterialImage resource="{res.ic_gwt_logo}" addStyleNames="{style.logo}"/>+
                </m:MaterialNavSection>

                <m:MaterialNavSection align="right">
                    <m:MaterialLink href="#about" iconPosition="left" icon="mdi-action-info-outline" text="About"
                                    textColor="blue"/>
                </m:MaterialNavSection>

                <m:MaterialNavSection align="right">
                    <m:MaterialLink href="#gettingStarted" iconPosition="left" icon="mdi-file-cloud-download"
                                    text="Getting Started" textColor="blue">
                        <m:MaterialBadge text="1.3.3" backgroundColor="blue"/>
                    </m:MaterialLink>
                </m:MaterialNavSection>

                <m:MaterialNavSection align="right">
                    <m:MaterialCollapsible>
                        <m:MaterialCollapsibleItem>
                            <m:MaterialCollapsibleHeader>
                                <m:MaterialLink text="Components" icon="mdi-action-view-quilt" textColor="blue"/>
                            </m:MaterialCollapsibleHeader>
                            <m:MaterialCollapsibleBody>
                                <m:UnorderedList>
                                    <m:ListItem>
                                        <m:MaterialLink href="#badges" text="Badges" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#buttons" text="Buttons" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#cards" text="Cards" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#chips" text="Chips" textColor="blue">
                                            <m:MaterialBadge text="New" backgroundColor="green"/>
                                        </m:MaterialLink>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#collapsibles" text="Collapsible" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#collections" text="Collections" textColor="blue"/>
                                    </m:ListItem>

                                    <m:ListItem>
                                        <m:MaterialLink href="#datagrid" text="Datagrid" textColor="blue">
                                            <m:MaterialBadge text="New" backgroundColor="green"/>
                                        </m:MaterialLink>
                                    </m:ListItem>

                                    <m:ListItem>
                                        <m:MaterialLink href="#dialogs" text="Dialogs" textColor="blue">
                                            <m:MaterialBadge text="Update" backgroundColor="blue"/>
                                        </m:MaterialLink>
                                    </m:ListItem>

                                    <m:ListItem>
                                        <m:MaterialLink href="#dropdowns" text="Dropdown" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#forms" text="Forms" textColor="blue">
                                            <m:MaterialBadge backgroundColor="blue" text="Update"/>
                                        </m:MaterialLink>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#footer" text="Footer" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#loaders" text="Loaders" textColor="blue">
                                            <m:MaterialBadge backgroundColor="blue" text="Update"/>
                                        </m:MaterialLink>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#media" text="Media" textColor="blue">
                                            <m:MaterialBadge text="Update" backgroundColor="blue"/>
                                        </m:MaterialLink>
                                    </m:ListItem>

                                    <m:ListItem>
                                        <m:MaterialLink href="#navigations" text="NavBar" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#scrollspy" text="Scrollspy" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#tabs" text="Tabs" textColor="blue">
                                            <m:MaterialBadge text="Update" backgroundColor="blue"/>
                                        </m:MaterialLink>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#transition" text="Transitions" textColor="blue"/>
                                    </m:ListItem>
                                </m:UnorderedList>
                            </m:MaterialCollapsibleBody>
                        </m:MaterialCollapsibleItem>

                        <m:MaterialCollapsibleItem>
                            <m:MaterialCollapsibleHeader>
                                <m:MaterialLink text="Style and Layout" icon="mdi-image-color-lens" textColor="blue"/>
                            </m:MaterialCollapsibleHeader>

                            <m:MaterialCollapsibleBody>
                                <m:UnorderedList>
                                    <m:ListItem>
                                        <m:MaterialLink href="#colors" text="Colors" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#grid" text="Grid" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#helper" text="Helper" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#icons" text="Icons" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#shadow" text="Shadow" textColor="blue"/>
                                    </m:ListItem>
                                    <m:ListItem>
                                        <m:MaterialLink href="#theme" text="Theming" textColor="blue"/>
                                    </m:ListItem>
                                </m:UnorderedList>
                            </m:MaterialCollapsibleBody>
                        </m:MaterialCollapsibleItem>
                    </m:MaterialCollapsible>
                </m:MaterialNavSection>

                <m:MaterialNavSection align="right">
                    <m:MaterialLink href="#showcase" iconPosition="left" icon="mdi-action-polymer" text="Showcase"
                                    textColor="blue">
                        <m:MaterialBadge backgroundColor="blue" text="Update"/>
                    </m:MaterialLink>
                </m:MaterialNavSection>

                <m:MaterialNavSection align="right">
                    <m:MaterialLink href="#templates" iconPosition="left" icon="mdi-navigation-apps" text="Templates"
                                    textColor="blue">
                        <m:MaterialBadge backgroundColor="green" text="New"/>
                    </m:MaterialLink>
                </m:MaterialNavSection>
                <m:MaterialNavSection align="right">
                    <m:MaterialLink href="#charts" iconPosition="left" icon="mdi-editor-insert-chart"
                                    text="Material Charts" textColor="blue">
                        <m:MaterialBadge backgroundColor="green" text="New"/>
                    </m:MaterialLink>
                </m:MaterialNavSection>
            </m:MaterialSideNav>
        </m:MaterialNavBar>

        <m:MaterialContainer>
            <!--THIS PANEL CONTAINS THE HEADER PRESENTER THAT SHOWS THE TITLE AND DESCRIPTION-->
            <g:SimplePanel ui:field="header"/>

            <g:HTMLPanel addStyleNames="{style.panel} container z-depth-2">
                <!--CHILD PRESENTERS GO IN THIS PANEL (see bindSlot() in ApplicationView.java)-->
                <g:SimplePanel ui:field="main"/>
            </g:HTMLPanel>
        </m:MaterialContainer>

        <m:MaterialFooter backgroundColor="blue" addStyleNames="{style.footer}">
            <m:MaterialRow>
                <m:MaterialColumn grid="s12 m4 m4">
                    <m:MaterialTitle fontSize="0.7" color="white" title="Support Us"
                                     description="Any amount would help support and continue development on this project and is greatly appreciated."/>
                    <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
                        <input type="hidden" name="cmd" value="_s-xclick"/>
                        <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----"/>
                        <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif"
                               name="submit" alt="PayPal - The safer, easier way to pay online!"/>
                        <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1"
                             height="1"/>
                    </form>
                    <m:MaterialLabel textColor="white" text="For bug bounties:"/>
                    <m:MaterialButton ui:field="btnBounty" text="Bountysource" backgroundColor="green" waves="light"/>
                </m:MaterialColumn>

                <m:MaterialColumn grid="s12 m4 l4">
                    <m:MaterialTitle fontSize="0.7" color="white" title="Join The Discussion"
                                     description="We provide Gitter Chat rooms in order for GWT Developers discussed and collaborate about GWT Material Design and Phonegap Integration."/>
                    <m:MaterialButton ui:field="btnChat" text="CHAT" backgroundColor="blue lighten-2" waves="light"/>
                </m:MaterialColumn>

                <m:MaterialColumn grid="s12 m4 l4">
                    <m:MaterialTitle fontSize="0.7" color="white" title="Github"
                                     description="Download, fork, pull, file issues, and more at the official GWT Material repo on Github."/>
                    <m:MaterialButton ui:field="btnGithub" text="Github" backgroundColor="blue lighten-2" waves="light"/>
                </m:MaterialColumn>
            </m:MaterialRow>

            <m:MaterialFooterCopyright backgroundColor="blue darken-1">
                <m:MaterialLabel text="© 2015 Copyright GWT Material"/>
            </m:MaterialFooterCopyright>
        </m:MaterialFooter>
    </g:HTMLPanel>
</ui:UiBinder>
