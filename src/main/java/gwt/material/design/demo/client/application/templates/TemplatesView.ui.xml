<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client">

    <ui:with field="res" type="gwt.material.design.demo.client.resources.MaterialResources" />

    <ma:masonry.MaterialMasonry marginTop="60">
        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage resource="{res.template_starter}"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Starter Template" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://gwtmaterialdesign.github.io/gwt-material-starter/snapshot/" target="_blank"/>
                    <m:MaterialLink text="Source" href="https://github.com/GwtMaterialDesign/gwt-material-template" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage resource="{res.template_wrenx}"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Map Template" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://wrenx2015.appspot.com/" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage url="https://camo.githubusercontent.com/fd44a45d475a7ccff65305dab9507ede1130b521/687474703a2f2f692e696d6775722e636f6d2f706661725147742e676966"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Google Plus Pattern" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://gwtmaterialdesign.github.io/gwt-material-patterns/snapshot/#google_plus" target="_blank"/>
                    <m:MaterialLink text="Source" href="https://github.com/GwtMaterialDesign/gwt-material-patterns/tree/master/src/main/java/com/github/gwtmaterialdesign/client/application/googleplus" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage url="https://camo.githubusercontent.com/c2a8dd8837c322cad255f2357426acd2404bac0a/687474703a2f2f692e696d6775722e636f6d2f5378684d7639322e676966"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Google Contacts Pattern" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://gwtmaterialdesign.github.io/gwt-material-patterns/snapshot#google_contacts" target="_blank"/>
                    <m:MaterialLink text="Source" href="https://github.com/GwtMaterialDesign/gwt-material-patterns/tree/master/src/main/java/com/github/gwtmaterialdesign/client/application/googlecontacts" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage url="https://camo.githubusercontent.com/9df0351340b8fc12350389abe9410d57712ec108/687474703a2f2f692e696d6775722e636f6d2f304d474c61596f2e676966"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Google Drive Pattern" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://gwtmaterialdesign.github.io/gwt-material-patterns/snapshot#google_drive" target="_blank"/>
                    <m:MaterialLink text="Source" href="https://github.com/GwtMaterialDesign/gwt-material-patterns/tree/master/src/main/java/com/github/gwtmaterialdesign/client/application/googledrive" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

        <m:MaterialColumn grid="s12 l4 m4">
            <m:MaterialCard backgroundColor="WHITE" >
                <m:MaterialCardImage waves="LIGHT" width="100%">
                    <m:MaterialImage url="https://camo.githubusercontent.com/e8135b9ee89260249589f718886b4baac0a383f6/687474703a2f2f692e696d6775722e636f6d2f763146324f6f792e676966"/>
                </m:MaterialCardImage>

                <m:MaterialCardContent>
                    <m:MaterialCardTitle text="Google Inbox Pattern" textColor="BLACK"/>
                </m:MaterialCardContent>

                <m:MaterialCardAction>
                    <m:MaterialLink text="Demo" href="http://gwtmaterialdesign.github.io/gwt-material-patterns/snapshot#google_inbox" target="_blank"/>
                    <m:MaterialLink text="Source" href="https://github.com/GwtMaterialDesign/gwt-material-patterns/tree/master/src/main/java/com/github/gwtmaterialdesign/client/application/googleinbox" target="_blank"/>
                </m:MaterialCardAction>
            </m:MaterialCard>
        </m:MaterialColumn>

    </ma:masonry.MaterialMasonry>
</ui:UiBinder>
