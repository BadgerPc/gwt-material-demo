                    <m:MaterialLabel textColor="BLUE" fontSize="5em" fontWeight="LIGHTER" text="5" />
                </m:MaterialPanel>
            </ma:carousel.MaterialCarousel>
            <demo:PrettyPre addStyleNames="lang-xml">
                &emsp;&lt;ma:carousel.MaterialCarousel textAlign="CENTER"><br/>
                &emsp;&lt;m:MaterialPanel paddingTop="50" backgroundColor="GREY_LIGHTEN_2" padding="20" height="200px"><br/>
                &emsp;&emsp;&lt;m:MaterialLabel textColor="BLUE" fontSize="5em" fontWeight="LIGHTER" text="1" /><br/>
                &emsp;&lt;/m:MaterialPanel><br/>
