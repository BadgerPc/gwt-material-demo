            <demo:PrettyPre addStyleNames="lang-java">
                &emsp;carousel.goToSlide(1) // Navigates to a slide by index <br/>
                carousel.next() // Navigates to the next slide <br/>
                carousel.previous() // Navigates to the previous slide <br/>
                carousel.pause() // Pauses autoplay <br/>
                carousel.play() // Starts autoplay <br/>
            </demo:PrettyPre>
