<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:m.addins="urn:import:gwt.material.design.addins.client.ui"
             xmlns:demo="urn:import:gwt.material.design.demo.client.ui">
    <m:MaterialPanel>
        <m:MaterialTitle title="Introduction" description="This component is an iframe that wraps your document using Google Doc Viewer. It's good for public document." />
        <m:MaterialRow padding="0">
            <m:MaterialColumn grid="l6 m12 s12" shadow="1" padding="0">
                <m:MaterialPanel backgroundColor="blue darken-1" width="100%" padding="8">
                    <m:MaterialLink backgroundColor="blue darken-1" text="File1.doc" textColor="white" iconType="NOTE" />
                </m:MaterialPanel>
                <m.addins:MaterialDocViewer width="100%" height="600px" url="http://infolab.stanford.edu/pub/papers/google.pdf" />
            </m:MaterialColumn>
        </m:MaterialRow>

        <demo:PrettyPre addStyleNames="lang-xml z-depth-1">
            &emsp;m.addins:MaterialDocViewer width="100%" height="400px" url="http://infolab.stanford.edu/pub/papers/google.pdf" />
        </demo:PrettyPre>
    </m:MaterialPanel>
</ui:UiBinder>
