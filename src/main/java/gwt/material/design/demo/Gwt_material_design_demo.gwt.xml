<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='gwt_material_design_demo'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>

    <inherits name="gwt.material.design.GwtMaterialDesign"/>

    <!-- Other module inherits                                      -->
    <inherits name="com.googlecode.gwt.charts.Charts"/>

    <!-- Specify the app entry point class.                         -->
    <entry-point class='gwt.material.design.demo.client.Gwt_material_design_demo'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>

</module>
